#include <iframe>

@cgmain

fn testcase:
    @m int n, k, r, c; @poll n k r c; --r; --c;
    for(i=0; i!=n; ++i)
        int j;
        for(j=0; j!=n; ++j)
            @raw ".X"[(i+j)%k==(r+c)%k]
        end
        @raw ln
    end
end

fn pub main -> int:
    int t; @poll t;
    while(t--) testcase end
end
