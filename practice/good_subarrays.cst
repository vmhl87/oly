#include <iframe>
#include <map>

@multi

int[1e5+1] ps;

fn inline test:
    int n; @poll n;

    for(i=0; i<n; ++i)
        byte a; @poll a;
        ps[i+1] = ps[i] + a - '0' - 1;
    end

    @map[int int] ct;
    ll res = 0;

    for(i=0; i<n+1; ++i)
        res = res + ct:get(ps[i]);
        ct:set(ps[i], ct:get(ps[i])+1);
    end

    @fmt res;
end
